    "log4js": "1.1.0",
    "moment": "2.15.0",
    "object-assign": "4.1.0",
    "rc-checkbox": "1.4.0",
    "rc-time-picker": "2.2.1",
    "react": "15.3.1",
    "react-dnd": "2.5.1",
    "react-dnd-html5-backend": "2.5.1",
    "react-dom": "15.3.1",
